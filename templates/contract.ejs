<% 
function extractParamTypes(methodName) {
  const paramTypes = {};
  const match = methodName.match(/\(([^)]+)\)/);
  if (match) {
    const params = match[1].split(',');
    params.forEach((param, index) => {
      const [type, name] = param.trim().split(' ');
      paramTypes[name || `param${index + 1}`] = type;
    });
  }
  return paramTypes;
}
